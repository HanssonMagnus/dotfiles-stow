! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

URxvt.intensityStyles:	false
URxvt.depth:		32
URxvt.font: 		xft:NotoSansMono-Regular:size=10
URxvt.scrollBar:	false
URxvt.cursorColor:	white
!URxvt.letterSpace: -1

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

! 100 is max transparency, and 0 is none. I had 20 for a long time.
URxvt.transparent: true
URxvt.shading:30

! Included this so you know how to make your foreground not match the color of your background
URxvt.foreground: #eeeeee

!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

! Shift Up to scroll one line higher
! Shift Down to scroll one line lower
! Control Left to jump to the previous word
! Control Right to jump to the next word
